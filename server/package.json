{
  "name": "saml-integration-tutorial-server",
  "version": "1.0.0",
  "description": "Server component of the `passport-saml` implementation.",
  "repository": "https://github.com/raphaelpreston/passport-saml-tutorial.git",
  "author": "Raphael Preston <raphael.o.preston@gmail.com>",
  "main": "src/server.js",
  "license": "MIT",
  "engines": {
    "node": "14.13.1"
  },
  "scripts": {
    "test": "eslint src/**.js",
    "dev": "nodemon src/server.js --exec babel-node",
    "start": "babel-node src/server.js",
    "build": "babel src -d dist",
    "prod": "yarn build; node dist/server.js"
  },
  "nodemonConfig": {
    "signal": "SIGHUP",
    "delay": "1500"
  },
  "dependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cryptr": "^6.0.2",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-saml": "^1.3.5"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.5",
    "nodemon": "^2.0.4"
  }
}
